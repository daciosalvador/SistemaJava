package cliente.aplicacao;

import java.util.Date;

import dao.ClientesFDAO;
import dto.ClientesDTO;

public class AddCliente {

	public static void main(String[] args) {

		ClientesFDAO clientesDAO = new ClientesFDAO();
		
		ClientesDTO clientesDTO = new ClientesDTO();
		clientesDTO.setNome_cliente("Dacio SALVADOR");
		clientesDTO.setCpf("456.455.456-52");
		clientesDTO.setCelular("55586665");
		clientesDTO.setData_cad_F(new Date());
		clientesDTO.getEnderecoDTO();
		
		
		clientesDAO.inserir(clientesDTO);

	}

}
